doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    title= title
    link(rel='stylesheet', href='/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet', href='/css/app.min.css')
    link(rel="shortcut icon", href="/public/img/icons/favicon.ico")
    each size in ['16x16', '32x32', '96x96']
        link(rel="icon", type="image/png", sizes=size, href="/img/icons/favicon-"+size+".png")
    each size in ['57x57', '60x60', '72x72', '76x76', '114x114', '120x120']
        link(rel="apple-touch-icon", sizes=size, href="/img/icons/apple-touch-icon-"+size+".png")
  body
    script(src='/jquery/dist/jquery.min.js')
    script(src='/lodash/dist/lodash.min.js')
    script(src='/queue-async/queue.min.js')
    script(src='/d3/d3.min.js')
    script(src='/react/react-with-addons.min.js')
    //- testing libs
    //- script(src='/react/JSXTransformer.js')
    block content
